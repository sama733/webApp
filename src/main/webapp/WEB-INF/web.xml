<!DOCTYPE web-app PUBLIC
        "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
    <display-name>Archetype Created Web Application</display-name>
    <servlet>
        <servlet-name>CreateRealCustomerServlet</servlet-name>
        <servlet-class>servlet.CreateRealCustomerServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SearchRealCustomerServlet</servlet-name>
        <servlet-class>servlet.SearchRealCustomerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>CreateRealCustomerServlet</servlet-name>
        <url-pattern>/CreateRealCustomerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SearchRealCustomerServlet</servlet-name>
        <url-pattern>/SearchRealCustomerServlet</url-pattern>
    </servlet-mapping>
</web-app>
